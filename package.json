{
  "name": "uiu",
  "version": "0.1.1",
  "description": "A development library, with 'Bit' at its foundation",
  "keywords": [
    "development library",
    "react components library",
    "js utilities library",
    "js helpers library",
    "scss functions library"
  ],
  "homepage": "https://dreamyguy.github.io/uiu/",
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamyguy/uiu"
  },
  "bugs": {
    "url": "https://github.com/dreamyguy/uiu"
  },
  "author": {
    "name": "Wallace Sidhrée",
    "url": "http://sidhree.com/"
  },
  "copyright": "Copyright (c) Wallace Sidhrée - MIT LICENSE",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "axios": "^0.21.1",
    "d3-scale-chromatic": "^2.0.0",
    "dotenv": "^8.2.0",
    "fork-me-on-github": "^1.0.6",
    "json-server": "^0.16.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-emojis": "^1.0.6",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prettier": "prettier --write '**/*.{js,css,scss,html,json,md}'",
    "server:json": "json-server --watch server/json/db.json",
    "start": "react-scripts start",
    "test": "react-scripts test --verbose",
    "test:travis": "react-scripts test"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^v14.12.0"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.20"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
